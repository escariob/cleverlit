  <title>Clever Lit</title>
    <link rel="icon" type="image/png" href="http://res.cloudinary.com/dtqzififb/image/upload/v1456229763/cleverlitC_kcsv53.png">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

    <% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
