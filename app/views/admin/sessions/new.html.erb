<div class="login-form">
<h1>Log In</h1>

<%= form_tag admin_sessions_path do %>
<p>
<%= label_tag :username %></br>
<%= text_field_tag :username %>
</p>

<p>
    <%= label_tag :password %></br>
    <%= password_field_tag :password_digest %>
</p>

<p>
    <%= submit_tag 'Log In' %>

</p>
<% end %>
 
</div>
